f = f[!f$id == '',]
f$new_id = cumsum(f$new_id)
label = f[, list(label=paste(get('feature'), collapse=' '),
id = unique(get('id'))), by= 'new_id']
label = label[, list(N = .N), by= 'label,id']
ord = order(-label$N)
label[ord,]
}
